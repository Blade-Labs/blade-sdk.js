{
  "name": "blade-sdk.js",
  "version": "0.5.0",
  "description": "A simple wrapper for Hedera SDK",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress --color --mode production",
    "publish": "node publish.js",
    "dev": "webpack --progress --color --watch --mode development",
