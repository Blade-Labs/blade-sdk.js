  ],
  "license": "ISC",
  "dependencies": {
    "@hashgraph/hethers": "^1.2.2",
    "@hashgraph/sdk": "^2.19.2",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "@types/node-fetch": "^2.6.2",
    "dotenv": "^16.0.3",
    "jest": "^29.3.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "prettier": "^2.8.3",
    "shelljs": "^0.8.5",
    "solc": "0.8.8",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4",
    "webpack-cli": "^4.10.0",
    "webpack": "^5.76.3"
