{
  "compilerOptions": {
    "outDir": "../dist/",
    "noImplicitAny": false,
    "module": "es2015",
    "target": "es5",
    "jsx": "react",
    "downlevelIteration": true,
    "allowJs": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "baseUrl": "../.",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "lib": [
      "dom",
      "es2015"
    ]
  },
  "include": ["../src/**/*"]
}
